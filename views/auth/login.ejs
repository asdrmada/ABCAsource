<% include ../partials/header %>

<form action='/login' method='POST'>
  <div class="form-group">
    <label for="exampleInputUser1">Username</label>
    <input type="username" class="form-control" id="exampleInputUser1" aria-describedby="usernameHelp" placeholder="Enter chosen username here!">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Enter chosen password here!">
  </div>
  <button type="Login here!" class="btn btn-primary">Submit</button>
</form>

<% include ../partials/footer %>